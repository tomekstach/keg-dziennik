{{!
Page footer.
}}
<!-- Start Clients Logo section -->
{{#displayclientlogoarea}}
<div class="clients-logo">
   <div class="container-fluid">
      <div class="row-fluid">
         <div id="client-logo" class="owl-carousel">
            <div class="item col-lg-4 col-md-6 col-sm-12">
               <img src="{{{clientlogo1}}}" alt="client-logo-1" />
            </div>
            <div class="item col-lg-4 col-md-6 col-sm-12">
               <img src="{{{clientlogo2}}}" alt="client-logo-2" />
            </div>
            <div class="item col-lg-4 col-md-6 col-sm-12">
               <img src="{{{clientlogo3}}}" alt="client-logo-3" />
            </div>
            <div class="item col-lg-4 col-md-6 col-sm-12">
               <img src="{{{clientlogo4}}}" alt="client-logo-4" />
            </div>
            <div class="item col-lg-4 col-md-6 col-sm-12">
               <img src="{{{clientlogo5}}}" alt="client-logo-5" />
            </div>
            <div class="item col-lg-4 col-md-6 col-sm-12">
               <img src="{{{clientlogo6}}}" alt="client-logo-6" />
            </div>
            <div class="item col-lg-4 col-md-6 col-sm-12">
               <img src="{{{clientlogo7}}}" alt="client-logo-7" />
            </div>
            <div class="item col-lg-4 col-md-6 col-sm-12">
               <img src="{{{clientlogo8}}}" alt="client-logo-8" />
            </div>
         </div>
      </div>
   </div>
</div>
{{/displayclientlogoarea}}
<!-- End Clients Logo section -->
<!-- Start Footer Section -->
{{#displayfooter}}
<footer id="page-footer">
   <div class="container-fluid">
      <div class="row-fluid">
         <div class="col-lg-3 col-md-3 col-sm-12">
            <img src="{{{footerlogo}}}" alt="footer-logo" style="margin-bottom:20px;" />
         </div>
         <div class="col-lg-3 col-md-3 col-sm-12">
            {{#footersection2heading}}
            <h5>{{{footersection2heading}}}</h5>
            {{/footersection2heading}}
            <ul class="common">
               {{#footersection2link1}}
               <li><a href="{{{footersection2link1url}}}">{{{footersection2link1}}}</a></li>
               {{/footersection2link1}}
               {{#footersection2link2}}
               <li><a href="{{{footersection2link2url}}}">{{{footersection2link2}}}</a></li>
               {{/footersection2link2}}
               {{#footersection2link3}}
               <li><a href="{{{footersection2link3url}}}">{{{footersection2link3}}}</a></li>
               {{/footersection2link3}}
               {{#footersection2link4}}
               <li><a href="{{{footersection2link4url}}}">{{{footersection2link4}}}</a></li>
               {{/footersection2link4}}
               {{#footersection2link5}}
               <li><a href="{{{footersection2link5url}}}">{{{footersection2link5}}}</a></li>
               {{/footersection2link5}}
            </ul>
         </div>
         <div class="col-lg-3 col-md-3 col-sm-12">
            {{#footersection3heading}}
            <h5>{{{footersection3heading}}}</h5>
            {{/footersection3heading}}
            <ul class="common">
               {{#footersection3link1}}
               <li><a href="{{{footersection3link1url}}}">{{{footersection3link1}}}</a></li>
               {{/footersection3link1}}
               {{#footersection3link2}}
               <li><a href="{{{footersection3link2url}}}">{{{footersection3link2}}}</a></li>
               {{/footersection3link2}}
               {{#footersection3link3}}
               <li><a href="{{{footersection3link3url}}}">{{{footersection3link3}}}</a></li>
               {{/footersection3link3}}
               {{#footersection3link4}}
               <li><a href="{{{footersection3link4url}}}">{{{footersection3link4}}}</a></li>
               {{/footersection3link4}}
               {{#footersection3link5}}
               <li><a href="{{{footersection3link5url}}}">{{{footersection3link5}}}</a></li>
               {{/footersection3link5}}
            </ul>
         </div>
         <div class="col-lg-3 col-md-3 col-sm-12">
            <h5>KONTAKT</h5>
            <ul>
               {{#footersection1content}}
               <li>{{{footersection1content}}}</li>
               {{/footersection1content}}
               {{#footersection1email}}
               <li><i class="fa fa-envelope-o" aria-hidden="true"></i><a href="mailto:{{{footersection1email}}}">{{{footersection1email}}}</a></li>
               {{/footersection1email}}
               {{#footersection1contactno}}
               <li><i class="fa fa-phone" aria-hidden="true"></i>{{{footersection1contactno}}}</li>
               {{/footersection1contactno}}
               {{#footersection1address}}
               <li><i class="fa fa-map-marker" aria-hidden="true"></i>{{{footersection1address}}}</li>
               {{/footersection1address}}
            </ul>
         </div>
      </div>
      <div class="clearfix"></div>
      <ul class="copyright">
         {{#hascopyright}}
         <li>
            <p class="copy">Copyright &copy; {{{copyrightY}}} {{{hascopyright}}} </p>
         </li>
         {{/hascopyright}}
      </ul>
   </div>
</footer>
{{/displayfooter}}
<!-- Start Bottom Footer section -->
<div class="bottom-footer">
   <!-- Start Custom Menu -->
   <nav class="nav navbar-nav d-md-none" aria-label="{{#str}}custommenu, admin{{/str}}">
      {{# output.custom_menu_flat }}
      <ul class="list-unstyled pt-3">
         {{> theme_boost/custom_menu_footer }}
      </ul>
      {{/ output.custom_menu_flat }}
   </nav>
   <!-- End Custom Menu-->
   {{{ output.standard_footer_html }}}
   {{{ output.standard_end_of_body_html }}}
</div>
<!-- Start Back To Top -->
{{#backtotop}}
<div id="backtotop" style="display: none;"> 
   <a class="scrollup" href="javascript:void(0);" title="Go to top">
   </a>
</div>
{{/backtotop}}
<!-- End Back To Top -->
</div>
<!-- End Bottom Footer section -->
<!-- End Footer Section -->